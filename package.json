{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "'API REST desenvolvida utilizando TypeScript'",
  "main": "./server/server.ts",
  "scripts": {
    "start": "PORT=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "PORT=5000 NODE_ENV=production node dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "Postgres"
  ],
  "engines": {
    "node": "~6.11.1",
    "npm": "~3.10.9"
  },
  "author": "Raphael Lima <raphael.aolima8@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.1.5",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "snyk": "^1.83.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.8",
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^3.5.1",
    "@types/express": "^4.0.35",
    "@types/http-status": "^0.2.29",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.88",
    "@types/passport": "^0.3.3",
    "@types/passport-jwt": "^2.0.19",
    "@types/sequelize": "^4.0.51",
    "@types/supertest": "^2.0.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.2.1",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nyc": "^10.3.2",
    "supertest": "^3.0.0",
    "testdouble": "^2.1.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  },
  "snyk": true
}
